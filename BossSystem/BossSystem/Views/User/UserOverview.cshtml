@model List<BossSystem.ViewModels.UserOverviewModel>

    <div class="container">
        <a asp-controller="Auth" asp-action="Register">Register new user</a>
        <br />
        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Username</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.Id</td>
                        <td>@user.Username</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
